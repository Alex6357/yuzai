{
  "name": "@yuzai/pannel-extension",
  "version": "0.1.0",
  "description": "Management pannel extension for Yuzai bot framework",
  "keywords": [
    "yuzai",
    "pannel",
    "management",
    "extension"
  ],
  "license": "GPL-3.0-or-later",
  "author": "Alex11",
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . & eslint ."
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.35.0",
    "@sveltejs/adapter-node": "^5.3.2",
    "@sveltejs/kit": "^2.39.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.3",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "less": "^4.4.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.38.10",
    "svelte-check": "^4.3.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5"
  },
  "peerDependencies": {
    "yuzai": "workspace:../.."
  },
  "publishConfig": {
    "access": "public"
  },
  "yuzai": {
    "needBuild": true,
    "buildScript": "build",
    "autoImport": true
  }
}
